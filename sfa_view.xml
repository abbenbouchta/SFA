<openerp>
    <data>
        
        
        
         <!--  Contrat -->
       
        <record id="view_contrat_form" model="ir.ui.view">
            <field name="name">sfp.contrat.form</field>
            <field name="model">sfp.contrat</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Contrat" version="7.0">
                    <sheet>
                      <group>
		                	<group>
				                <field name="name" />
				                <field name="entreprise" />
				        
				                <field name="type" />

				            </group>
			                <group>
			                    <field name="apprenti" />
			                    <field name="vacataire" />
			                    <field name="maitre" />

			                </group>
		               </group>
		               <newline/>
		               <group>
		                	<group>
		                	    
				                <field name="groupe" />
				                <field name="metier" />
				                <field name="parent_link" placeholder="cas de declaration"/>
				                
				            </group>
			                <group>
			                	<field name="responsable" placeholder="si mineur"/>
			                	<field name="user" />
			                	<field name="relation" />
			                </group>
		               </group>
		               <newline/>
		               <group>
		                	<group>
				                
				                <field name="cfa" />
				                <field name="annexe" readonly="True"/>
				                <field name="date_start" />
				                
				            </group>
			                <group>
			                	
			                	
			                	<field name="date_end" />
			                	<field name="etat" />
			                	<field name="date_abandon" />
			                <!-- <field name="user" />-->
			                	
			                	
			                </group>
		               </group>
		                <notebook>
		               <page string='Factures'>
		               <field name='invoice_ids'/>
		               </page>
		               
		               </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_contrat_tree" model="ir.ui.view">
            <field name="name">sfp.contrat.tree</field>
            <field name="model">sfp.contrat</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Contrat">
                    <field name="name" />
                    <field name="type" />
                    <field name="description" />
                </tree>
            </field>
        </record>


        <record id="action_view_contrat" model="ir.actions.act_window">
            <field name="name">Contrat </field>
            <field name="res_model">sfp.contrat</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        
        
