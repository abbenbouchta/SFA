<openerp>
    <data>
        
        
        
         <!--  Contrat -->
       
        <record id="view_contrat_form" model="ir.ui.view">
            <field name="name">sfp.contrat.form</field>
            <field name="model">sfp.contrat</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Contrat" version="7.0">
                    <sheet>
                      <group>
		                	<group>
				                <field name="name" />
				                <field name="entreprise" />
				        
				                <field name="type" />

				            </group>
			                <group>
			                    <field name="apprenti" />
			                    <field name="vacataire" />
			                    <field name="maitre" />

			                </group>
		               </group>
		               <newline/>
		               <group>
		                	<group>
		                	    
				                <field name="groupe" />
				                <field name="metier" />
				                <field name="parent_link" placeholder="cas de declaration"/>
				                
				            </group>
			                <group>
			                	<field name="responsable" placeholder="si mineur"/>
			                	<field name="user" />
			                	<field name="relation" />
			                </group>
		               </group>
		               <newline/>
		               <group>
		                	<group>
				                
				                <field name="cfa" />
				                <field name="annexe" readonly="True"/>
				                <field name="date_start" />
				                
				            </group>
			                <group>
			                	
			                	
			                	<field name="date_end" />
			                	<field name="etat" />
			                	<field name="date_abandon" />
			                <!-- <field name="user" />-->
			                	
			                	
			                </group>
		               </group>
		                <notebook>
		               <page string='Factures'>
		               <field name='invoice_ids'>
		                 <field name="name" />
		                 <field name="metier" />
		                 <field name="apprenti" />
		                </field>
		               </page>
		               
		               </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_contrat_tree" model="ir.ui.view">
            <field name="name">sfp.contrat.tree</field>
            <field name="model">sfp.contrat</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Contrat">
                    <field name="name" />
                    <field name="type" />
                    <field name="description" />
                </tree>
            </field>
        </record>


        <record id="action_view_contrat" model="ir.actions.act_window">
            <field name="name">Contrat </field>
            <field name="res_model">sfp.contrat</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        
        
         <!-- Tuteurs-->
       
        <record id="form_view_tuteur_form" model="ir.ui.view">
            <field name="name">sfp.tuteur.form</field>
            <field name="model">sfp.tuteur</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Tuteur" version="7.0">
                    <sheet>
                      <group>
		                	<group>
				                <field name="name_ar" />
				                <field name="cin_ar" />
				            </group>
			                <group>
			                	<field name="last_name_ar" />
                                <field name="tel_ar" />
			                </group>
			                
			                <group>
			                    <field name="date_birth" />
				                <field name="na_ar" />
				                <field name="profession_ar" />
				                
				            </group>
			                <group>
			                    <field name="lieu_ar" />
			                	<field name="adresse_ar" />
			                	
			                </group>
		               </group>
                    <newline/>
	                	<group colspan="2" string="Description"  >
	                		<field name="description" nolabel="1" />
	                	</group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="tree_view_tuteur_tree" model="ir.ui.view">
            <field name="name">sfp.tuteur.tree</field>
            <field name="model">sfp.tuteur</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Tuteur">
                    <field name="name_ar" />
                    <field name="last_name_ar" />
                    <field name="cin_ar" />
                    <field name="tel_ar" />
                    <field name="date_birth" />
                </tree>
            </field>
        </record>


        <record id="action_view_tuteur" model="ir.actions.act_window">
            <field name="name">Tuteur </field>
            <field name="res_model">sfp.tuteur</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        
         <!-- Maitre-->
       
        <record id="form_view_maitre_form" model="ir.ui.view">
            <field name="name">sfp.maitre.form</field>
            <field name="model">sfp.maitre</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Maitre" version="7.0">
                    <sheet>
                      <group>
		                	<group>
				                <field name="name" />
				                <field name="name_ar" />

				            </group>
			                <group>
			                	<field name="last_name" />
			                	<field name="last_name_ar" />

			                </group>
			                
			                <group>
				                <field name="cin" />
				                <field name="n_entreprise" />
				                <field name="gender" />
				                
				            </group>
			                <group>
			                    <field name="age" />
			                    <field name="encadrant" />
			                	<field name="tel" />
			                	
			                </group>
		               </group>
                    <newline/>
	                	<group colspan="2" string="Description"  >
	                		<field name="description" nolabel="1" />
	                	</group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="tree_view_maitre_tree" model="ir.ui.view">
            <field name="name">sfp.maitre.tree</field>
            <field name="model">sfp.maitre</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Maitre">
                    <field name="name" />
                    <field name="name_ar" />
                    <field name="last_name_ar" />
                    <field name="cin" />
                    <field name="tel" />
                    <field name="age" />
                </tree>
            </field>
        </record>


        <record id="action_view_maitre" model="ir.actions.act_window">
            <field name="name">Maitre </field>
            <field name="res_model">sfp.maitre</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
	   
	   <!-- apprenti --> 
	   
         <record id="sfp_entreprise_search_apprenti" model="ir.ui.view">
	            <field name="name">sfp.apprenti.search</field>
	            <field name="model">sfp.apprenti</field>
	            <field name="type">search</field>
	            <field name="arch" type="xml">
	                <search string="Search Partner">
	                   <group col='10' colspan='4'>
	                        <field name="name" select="1" filter_domain="['|',('name','ilike',self),('first_name','ilike',self)]" />
	                     	<field name="cin"/>
	                   </group>
	                   <newline />
	                   <group expand="0" string="Group By...">
	                       <filter string="Salesman" icon="terp-personal" domain="[]" context="{'group_by' : 'cin'}" />
	                   </group>
	               </search>
	            </field>
	        </record>
	    
   
	    
	    <!-- form -->
	    
	    <record id="sfp_entreprise_form_apprenti" model="ir.ui.view">
	            <field name="name">sfp.apprenti.form</field>
	            <field name="model">sfp.apprenti</field>
	            <field name="type">form</field>
	            <field name="arch" type="xml">
	                <form string="Entreprise" version="7.0">
	                <sheet>
	                	<group>
	                	    <group>
                				<p align="left"><field name="image" widget="image" nolabel="1" width="130" /></p>
                			</group>
                		</group>
                		
	                	<newline/>
	                    <group >
	                        <group >
	                            <field name="name" />
	                            <field name="name_arabic" />  
	                            <field name="birthdate" />       
	                            <field name="age" />       
	                        </group>
	                        <group>
	                            <field name="cin" /> 
	                        	<field name="first_name_arabic" />
	                        	<field name="lieu_birth" /> 
	                        	<field name="gender" />
	                        	
	                        </group>
	                  	</group>
	                  	<newline/>
		               
		               <!-- for apprenti -->
	                    <group string="Informations Apprenti">
	                    	<group>
	                            <field name="function" />
	                            <field name="nv_scolaire" />
	                            <!-- <field name="dure_formation" />-->
	                            <field name="profession_tuteur_ar" />
	                        </group>
	                        <group>
	                            <field name="diplom"/>
	                            <field name="function_ar" />
	                            <field name="situation_ar" />
	                          <!--   <field name="parental_ar"/>-->
	                        </group>
	                    </group>
	                 <notebook colspan="4">
	                    <page string="Coordonnées">
								<group>
									<group>
			                            <field name="phone"/>
			                            <field name="email" required="context.get('force_email', False)"/>
			                            <field name="street"/>
			                            <field name="zip"/>
			                            <field name="country_id"/>
			                            
			                        </group>
									<group>
										<field name="mobile"/>
										<field name="fax"/>
										<field name="street2"/>
										<field name="city"/>
										<field name="website"/>
									</group>
								</group>
	                        </page>

	                    </notebook>
	                </sheet>
	                </form>
	            </field>
	        </record>
	    
	    <!-- tree -->
	    
	    <record model="ir.ui.view" id="sfp_entreprise_tree_apprenti">
	            <field name="name">sfp.apprenti.tree</field>
	            <field name="model">sfp.apprenti</field>
	            <field name="type">tree</field>
	            <field name="arch" type="xml">
	                <tree string="Apprenti">
	                    <field name="name"/>
	                    <field name="cin" />
	                    <field name="phone"/>
	                    <field name="email"/>
	                    <field name="city"/>
                        
	                </tree>
	            </field>
	    </record>
	    
	    
    <!-- Kanban view -->
    
		<record model="ir.ui.view" id="sfp_kanban_view_apprenti">
			<field name="name">apprenti kanban</field>
			<field name="model">sfp.apprenti</field>
			<field name="arch" type="xml">
				<kanban>
					<field name="name"/>
					<templates>
						<t t-name="kanban-box">
							<t t-set="color"
								t-value="kanban_color(record.name.raw_value || record.name.raw_value)" />
							<div
								t-att-class="color + (record.name.raw_value == 1 ? ' oe_kanban_color_alert' : '')">
								<div class="oe_kanban_box oe_kanban_color_border">
									<div
										class="oe_kanban_box_header oe_kanban_color_bgdark oe_kanban_color_border oe_kanban_draghandle">
										<table class="oe_kanban_table">
											<tr>
												<td class="oe_kanban_title1" align="left" valign="middle"
													style="padding-left:10px;">
													<h4>
														<a type="open">
															<field name="name"/>
														</a>
													</h4>
												</td>
											</tr>
										</table>
									</div>
									<div
										class="oe_kanban_box_content oe_kanban_color_bglight oe_kanban_box_show_onclick_trigger oe_kanban_color_border">
										<table class="oe_kanban_table">
											<tr width="300">
												<td valign="top" width="64" align="left">
													<a type="open">
														<img heigh="80" width="80"
															t-att-src="kanban_image('res.partner', 'photo', record.id.value)"/>
													</a>
												</td>
												<td valign="top" align="left" style="padding-left:10px;">
													<table>
														<tr>
															<td>Name :</td>
															<td style="padding-left:10px;">
																<b>
																	<field name="name"/>
																</b>
															</td>
														</tr>
														<tr>
															<td>CIN: </td>
															<td style="padding-left:10px;">
																<b>
																	<field name="cin"/>
																</b>
															</td>
														</tr>
														
													</table>
												</td>
											</tr>
										</table>
									</div>
									<div
										class="oe_kanban_buttons_set oe_kanban_color_border oe_kanban_color_bglight oe_kanban_box_show_onclick">
										<div class="oe_kanban_left">
											<a string="Edit" icon="gtk-edit" type="edit" />
											<a string="Change Color" icon="color-picker" type="color"
												name="color"/>
										</div>
										<br class="oe_kanban_clear"/>
									</div>
								</div>
							</div>
						</t>
					</templates>
				</kanban>
			</field>
		</record>

